TITLE: STUDY 4_Adoption
DATA: FILE = Study 4_Model.dat;
VARIABLE: NAMES = MNOS SKEP SCN SUP ADOPT PA NA;
          USEVAR = MNOS SKEP SCN ADOPT ; 
          !USEVAR = MNOS PA NA ADOPT; !WHEN TESTING THE MEDIATING ROLES OF PA AND NA
          COUNT = ADOPT;

ANALYSIS: 
ESTIMATOR = ML;
ALGORITHM=ODLL;
!BOOTSTRAP = 5000; 
!THE BOOTSTRAP OPTION WAS USED TO OBTAIN BIAS CORRECTED BOOTSTRAP CONFIDENCE INTERVALS TO EVALUATE INDIRECT EFFECTS!

MODEL:

SKEP ON MNOS(a1);
SCN ON MNOS(a2);
ADOPT ON SKEP(b1)
         SCN(b2)
         MNOS;
SKEP WITH SCN;

MODEL INDIRECT:
ADOPT IND MNOS;


!MODEL CONSTRAINT:
!NEW (MED1 MED2);
!MED1 = a1*b1;
!MED2 = a2*b2;
!MODEL TEST:
!0=MED1+MED2;

!PA ON MNOS;
!NA ON MNOS;
!ADOPT ON PA 
!         NA 
!         MNOS;
!PA WITH NA;
!MODEL INDIRECT:
!ADOPT IND MNOS;


OUTPUT: SAMPSTAT STDYX CINTERVAL;!(BCBOOTSTRAP); 
!SAMPSTAT STDYX CINTERVAL(BCBOOTSTRAP); !THE CINTERVAL(BCBOOTSTRAP) OPTION WAS USED TO OBTAIN BIAS CORRECTED BOOTSTRAP CONFIDENCE INTERVALS TO EVALUATE INDIRECT EFFECTS!